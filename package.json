{
  "name": "markdown-editor",
  "version": "0.0.2",
  "scripts": {
    "pack": "run-s private:prebuild private:copy private:compile:prod private:install private:pack",
    "debug": "run-s private:prebuild private:copy private:compile:dev private:install private:debug",
    "private:debug": "electron ./app --debug",
    "private:pack": "node scripts/pack.js",
    "private:compile:dev": "cross-env NODE_ENV=development run-p private:compile:main private:compile:renderer",
    "private:compile:prod": "cross-env NODE_ENV=production run-p private:compile:main private:compile:renderer",
    "private:compile:main": "node --trace-warnings scripts/compile/main.js",
    "private:compile:renderer": "node --trace-warnings scripts/compile/renderer.js",
    "private:copy": "node scripts/copy.js",
    "private:install": "node scripts/install.js",
    "private:prebuild": "node app/frame/constants/ipcMessages.genrator.js"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "highlight.js": "^9.11.0",
    "katex": "^0.7.1",
    "markdown-it": "^8.3.1",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-checkbox": "^1.1.0",
    "markdown-it-deflist": "^2.0.1",
    "markdown-it-footnote": "^3.0.1",
    "markdown-it-ins": "^2.0.0",
    "markdown-it-mark": "^2.0.0",
    "markdown-it-math": "^4.0.1",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-toc": "^1.1.0",
    "mobx": "^3.1.9",
    "mobx-react": "^4.1.8",
    "monaco-editor": "^0.8.3",
    "react-addons-pure-render-mixin": "^15.5.2",
    "react-dnd": "^2.3.0",
    "react-dnd-html5-backend": "^2.3.0"
  },
  "devDependencies": {
    "@types/react": "^15.0.24",
    "@types/react-addons-pure-render-mixin": "^0.14.16",
    "@types/react-dnd": "^2.0.32",
    "@types/react-dnd-html5-backend": "^2.1.7",
    "@types/react-dom": "^15.5.0",
    "awesome-typescript-loader": "^3.1.3",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-plugin-transform-es5-property-mutators": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.1",
    "electron": "^1.6.6",
    "electron-packager": "^8.7.0",
    "fs-extra": "^3.0.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "npm-run-all": "^4.0.2",
    "style-loader": "^0.17.0",
    "typescript": "^2.3.2",
    "uglify-js": "git+https://github.com/mishoo/UglifyJS2.git#harmony",
    "webpack": "^2.5.0"
  }
}
